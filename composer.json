{
  "name": "codicastudio/impersonate",
  "description": "A noobs worthless garbage, just learning.",
  "keywords": [
      "worthless garabage by a noob."
  ],
  "homepage": "https://github.com/codicastudio/impersonate",
  "license": "MIT",
  "authors": [
      {
          "name": "Michael J Brown",
          "email": "codicastudiodev@gmail.com",
          "homepage": "https://codicastudio.dev",
          "role": "Developer"
      }
  ],
  "require": {
    "php": "^7.4.8",
    "laravel/framework": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^5.0",
    "orchestra/database": "^5.0",
    "symfony/mime": ">=4.3.8",
    "symfony/http-foundation": ">=4.3.8"
  },
  "autoload": {
    "psr-4": {
      "codicastudio\\Impersonate\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "codicastudio\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
},
  "extra": {
    "laravel": {
      "providers": [
        "codicastudio\\Impersonate\\ImpersonateServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
